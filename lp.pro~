%number one
sum-up-numbers-simple([], 0).
sum-up-numbers-simple([H|T], N):-
    number(H),
    sum-up-numbers-simple(T,Value),
    N is H+Value.

sum-up-numbers-simple([H|T], N):-
    \+number(H),
    sum-up-numbers-simple(T,N).



